<h1>タスク一覧</h1>
<table>
    <thead>
        <tr>
            <th>タスク名</th>
        </tr>
    </thead>
    <tbody>
        <% @tasks.each do |tasks| %>
            <tr>
                <td><%= task.title %></td>
                <td><%= link_to '編集, edit_tasks_path'%></td>
            </tr>
        <% end %>
    </tbody>
</table>