<h1>タスク一覧</h1>
<table>
    <thead>
        <tr>
            <th>タスク名</th>
        </tr>
    </thead>
    <tbody>
        <% @tasks.each do |tasks| %>
            <tr>
                <td><%= tasks.title %></td>
                <td><%= %></td>
            </tr>
        <% end %>
    </tbody>
</table>
<%= link_to '追加',new_task_path %>